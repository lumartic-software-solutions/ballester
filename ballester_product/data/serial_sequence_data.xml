<odoo>
    <data>
        <!-- Sequences for stock.production.lot in lot number -->
        <record id="seq_stock_move_line_itm" model="ir.sequence">
            <field name="name">Sequence Of Lot Serial Number </field>
            <field name="code">ballester.stock.production.lot</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="prefix">%(range_year)s%(month)s%(day)s</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
