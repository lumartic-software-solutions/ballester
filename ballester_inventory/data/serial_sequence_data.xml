<odoo>
    <data>
        <!-- Sequences for stock.move.line in lot number -->
        <record id="seq_stock_move_line_itm" model="ir.sequence">
            <field name="name">Lot Serial Sequence</field>
            <field name="code">stock.move.line</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
