<odoo>
    <data noupdate="1">
        <!-- Sequences for account for customer -->
        <record id="seq_customer_account_sequnce" model="ir.sequence">
            <field name="name">Customer Sequence</field>
            <field name="code">account.customer</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="prefix">43</field>
             <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for account for supplier -->
        <record id="seq_supplier_account_sequnce" model="ir.sequence">
            <field name="name">Supplier Sequence</field>
            <field name="code">account.supplier</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="prefix">40</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for account for supplier service -->
        <record id="seq_supplier_service_account_sequnce" model="ir.sequence">
            <field name="name">Supplier service Sequence</field>
            <field name="code">account.service</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="prefix">41</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
